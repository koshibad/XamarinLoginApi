<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MOB.XF.Login.View.HomeView"
             x:Title="Home">

  <ContentPage.Content>
    <StackLayout Padding="20" x:Name="rootLayout">
      <Button x:Name="btnVoltar"
                Text="Voltar"
					      Clicked="Voltar_Clicked" />
      <Button x:Name="btnListar"
                Text="Listar"
					      Clicked="Listar_Clicked" />
      <Button x:Name="btnEditar"
                Text="Editar"
					      Clicked="Editar_Clicked" />
    </StackLayout>
  </ContentPage.Content>
  
</ContentPage>